<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/self_include"
        layout="@layout/activity_profile"
        android:layout_width="403dp"
        android:layout_height="500dp"
        android:layout_marginTop="33dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/review_self"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_profile" />

    <Button
        android:id="@+id/edit_profile"
        android:layout_width="69dp"
        android:layout_height="48dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="33dp"
        android:text="edit"
        app:layout_constraintBottom_toTopOf="@+id/self_include"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/find_others"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/review_self"
        android:layout_width="wrap_content"
        android:layout_height="37dp"
        android:layout_marginStart="176dp"
        android:layout_marginEnd="147dp"
        android:layout_marginBottom="30dp"
        android:text="review"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/self_include" />

    <TextView
        android:id="@+id/find_others"
        android:layout_width="188dp"
        android:layout_height="32dp"
        android:layout_marginStart="71dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="13dp"
        android:text="find others"
        app:layout_constraintBottom_toTopOf="@+id/self_include"
        app:layout_constraintEnd_toStartOf="@+id/edit_profile"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>